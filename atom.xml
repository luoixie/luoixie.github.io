---
layout: blank
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
   <title>{{ site.title }}</title>
   <link href="https://luoixie.github.io{{ site.feed }}" rel="self" type="application/atom+xml"/>
   <link href="https://luoixie.github.io" rel="alternate" type="text/html" />
   <updated>{{ site.time | date_to_xmlschema }}</updated>
   <id>https://luoixie.github.io</id>
   <author>
     <name>{{ site.author.name }}</name>
     <email>{{ site.author.email }}</email>
   </author>

   {% for post in site.posts limit:10 %}
   <entry>
     <title>{{ post.title }}</title>
     <link href="https://luoixie.github.io{{ post.url }}"/>
     <updated>{{ post.date | date_to_xmlschema }}</updated>
     <id>https://luoixie.github.io
{{ post.id }}</id>
     <content type="html">{{ post.content | xml_escape }}</content>
   </entry>
   {% endfor %}
 
</feed>

